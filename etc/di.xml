<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="LokiCheckout\Core\Config\LokiCheckoutModules">
        <arguments>
            <argument name="modules" xsi:type="array">
                <item name="LokiCheckout_PaypalExpress" xsi:type="string">LokiCheckout_PaypalExpress</item>
            </argument>
        </arguments>
    </type>

    <type name="LokiCheckout\PaypalExpress\Payment\Redirect\RedirectResolver">
        <arguments>
            <argument name="session" xsi:type="object">Magento\Paypal\Model\Session</argument>
        </arguments>
    </type>
</config>
